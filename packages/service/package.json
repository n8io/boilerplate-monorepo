{
  "name": "@boilerplate-monorepo/service",
  "version": "1.4.0",
  "description": "Awesome project developed with TypeORM.",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^14.2.0",
    "yarn": "^1.19.1"
  },
  "scripts": {
    "build": "babel src -d dist --ignore '**/*.spec.js' --source-maps true",
    "migrate": "babel-node node_modules/.bin/knex",
    "migration:down": "yarn -s migrate migrate:down",
    "migration:gen": "yarn -s migrate migrate:make",
    "migration:latest": "yarn -s migrate migrate:latest",
    "migration:show": "yarn -s migrate migrate:list",
    "migration:up": "yarn -s migrate migration:up",
    "postbuild": "node ./.postbuild",
    "prebuild": "node ./.prebuild",
    "publish:service": "apollo service:push --tag local --endpoint=https://local.host:4000/graphql",
    "start": "yarn -s watch",
    "start:prod": "node -r dotenv/config dist",
    "test": "jest --passWithNoTests",
    "test:ci": "JEST_JUNIT_OUTPUT_DIR=../../.tmp/test-results/jest/service  yarn -s test --ci --runInBand --reporters=default --reporters=jest-junit",
    "test:watch": "jest --passWithNoTests --color --watch",
    "watch": "nodemon"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  },
  "dependencies": {
    "@boilerplate-monorepo/common": "^1.4.0",
    "@sentry/node": "^5.15.5",
    "apollo-server-cache-redis": "^1.1.5",
    "apollo-server-express": "^2.9.15",
    "bcryptjs": "^2.4.3",
    "bookshelf": "^1.1.0",
    "bookshelf-case-converter-plugin": "^1.0.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "dataloader": "^2.0.0",
    "date-fns": "^2.8.1",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "graphql-middleware-sentry": "^3.2.1",
    "graphql-rate-limit": "^2.0.1",
    "graphql-rate-limit-directive": "^1.2.1",
    "graphql-scalars": "^1.1.0",
    "graphql-voyager": "^1.0.0-rc.29",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "merge-graphql-schemas": "^1.7.6",
    "mock-knex": "^0.4.8",
    "ms": "^2.1.2",
    "nodemailer": "^6.4.6",
    "pg": "^8.0.3",
    "ramda": "^0.27.0",
    "rate-limiter-flexible": "^2.1.3",
    "redis": "^3.0.2",
    "speakeasy": "^2.0.0",
    "sqlite3": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@sentry/cli": "^1.52.3",
    "apollo-server-testing": "^2.12.0",
    "babel-jest": "^24.8.0",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-jest": "^23.10.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-ramda": "^2.5.1",
    "graphql-tools": "^5.0.0",
    "html-minifier": "^4.0.0",
    "jest": "^25.5.0",
    "jest-watch-typeahead": "^0.6.0",
    "nodemon": "^2.0.2",
    "testdouble": "^3.13.1",
    "testdouble-jest": "^2.0.0",
    "update-dotenv": "^1.1.1"
  }
}
